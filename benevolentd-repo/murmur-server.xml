<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Mumble</Name>
  <Description>
    Mumble is an open source, low-latency, high quality voice chat software primarily intended for use while gaming.[br][br]
    [b][span style='color: #E80000;']Directions:[/span][/b][br]
    [b]/config[/b] : Mumble will store it's configuration files here.
   </Description>
  <Registry>https://registry.hub.docker.com/brentwood/docker-mumble/</Registry>
  <GitHub>https://github.com/benevolentd/docker-containers/tree/master/mumble/</GitHub>
  <Repository>benevolentd/docker-mumble</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>64738</HostPort>
        <ContainerPort>64738</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>64738</HostPort>
        <ContainerPort>64738</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment/>
  <Data>
    <Volume>
      <HostDir>/path/to/docker/mumble</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI></WebUI>
  <Banner>http://i.imgur.com/5vSV3Gk.png</Banner>
  <Icon>http://i.imgur.com/5vSV3Gkl.png</Icon>
</Container>
